{
  "private": true,
  "name": "@softwareventures/revolut-feed",
  "version": "0.0.0-development",
  "description": "Experimental feed of transactions from Revolut",
  "keywords": [
    "revolut",
    "banking",
    "feed"
  ],
  "contributors": [
    "Roxanne Gibson <me@roxanne.dev>",
    "Daniel Cassidy <mail@danielcassidy.me.uk>"
  ],
